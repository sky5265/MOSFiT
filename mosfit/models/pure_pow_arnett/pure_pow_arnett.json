{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "tpeak":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"t_{\\rm peak}"
    },
    "lumscale":{
        "kind":"parameter",
        "value":1.0e44,
        "class":"parameter",
        "latex":"\\L_{\\rm scale}"
    },
    "alpha":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"\\alpha"
    },
    "kappa_pow":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"\\kappa_{pow}\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "vejecta_pow":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm pow,ej}\\,({\\rm km\\,s}^{-1})"
    },
    "temperature_pow":{
        "kind":"parameter",
        "value":5.0e3,
        "class":"parameter",
        "latex":"T_{exp~\\min}\\,{\\rm (K)}"
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{1~\\min}\\,{\\rm (K)}"
    },
    "fnickel":{
        "kind":"parameter",
        "value":1.0e-1,
        "class":"parameter",
        "latex":"f_{1~\\rm Ni}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.15,
        "class":"parameter",
        "latex":"\\kappa_{12}\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\_{12},({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "m1":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_1, (M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{~1\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "m3":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "m13":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "m1",
                "m3"
        ],
        "inputs":[
                "m1",
                "m3"
        ],
        "result":"m13",
        "class":"operator"
    },
    "pure_pow":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "tpeak",
            "lumscale",
            "texplosion",
            "alpha"
        ],
        "replacements":{
            "luminosities":"pure_pow_luminosities"
        }
    },
    "temperature_floor_pow":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "pure_pow",
            "temperature_pow",
            "vejecta_pow",
            "kappa_pow"
        ],
        "replacements":{
            "luminosities":"pure_pow_luminosities",
            "temperature":"temperature_pow",
            "vejecta":"vejecta_pow",
            "kappa":"kappa_pow"
        }
    },
    "blackbody_pow":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor_pow"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"pure_pow_luminosities"
        }
    },
    "nickelcobalt":{
        "kind":"engine",
        "class":"nickelcobalt",
        "inputs":[
            "densetimes",
            "m1",
            "fnickel",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities",
            "mejecta":"m1"
        }
    },
    "diffusion":{
        "kind":"transform",
        "class":"diffusion",
        "inputs":[
            "m1",
            "m3",
            "nickelcobalt",
            "kappa",
            "kappagamma",
            "m13",
            "texplosion",
            "vejecta"
        ],
        "replacements":{
            "mejecta":"m13",
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "diffusion",
            "temperature"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "blackbody1":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor",
            "blackbody_pow"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody1",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "losextinction",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    }
}
