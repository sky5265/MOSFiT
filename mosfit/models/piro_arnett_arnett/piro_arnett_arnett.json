{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "Re":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"r_e"
    },
    "vt":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"v_t"
    },
    "Me":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_e"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature1":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "fnickel1":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "kappa1":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma1":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "m1":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta1":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "temperature2":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "fnickel2":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "kappa2":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma2":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta2":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta2":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "mejecta1":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "m1",
                "mejecta2"
        ],
        "inputs":[
                "m1",
                "mejecta2"
        ],
        "result":"mejecta1",
        "class":"operator"
        
    },
    "m_total_diffusion":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "mejecta1",
                "Me"
        ],
        "inputs":[
                "mejecta1",
                "Me"
        ],
        "result":"m_total_diffusion",
        "class":"operator"
    },
    "nickelcobalt1":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta1",
            "fnickel1",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities"
        }
    },
    "diffusion1":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt1",
            "kappa1",
            "kappagamma1",
            "m_total_diffusion",
            "texplosion",
            "vejecta1"
        ],
        "replacements":{
            "mejecta":"m_total_diffusion",
            "luminosities":"nickel_cobalt1_luminosities"
        }
    },
    "temperature_floor1":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion1",
            "temperature1"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities"
        }
    },
    "blackbody1":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor1"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities"
        }
    },
    "nickelcobalt2":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta2",
            "fnickel2",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt2",
            "kappa2",
            "kappagamma2",
            "m_total_diffusion",
            "texplosion",
            "vejecta2"
        ],
        "replacements":{
            "mejecta":"m_total_diffusion",
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion2",
            "temperature2"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "shockcoolingpiro":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "Me",
            "Re",
            "vt",
            "kappa",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "piro_expanding_cooling_shock":{
        "kind":"photosphere",
        "inputs":[
            "shockcoolingpiro",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "blackbody3":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "redshift",
            "piro_expanding_cooling_shock",
            "blackbody2"
        ],
        "requests":{
            "seds":"blackbody2",
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody2",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "losextinction",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    }
}
