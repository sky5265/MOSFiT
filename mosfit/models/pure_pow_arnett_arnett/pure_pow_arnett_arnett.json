{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "tpeak":{
        "kind":"parameter",
        "value":100.0,
        "class":"parameter",
        "latex":"t_{\\rm peak}"
    },
    "lumscale":{
        "kind":"parameter",
        "value":1.0e44,
        "class":"parameter",
        "latex":"\\L_{\\rm scale}"
    },
    "alpha":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"\\alpha"
    },
    "kappa_pow":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"\\kappa_{pow}\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "vejecta_pow":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm pow,ej}\\,({\\rm km\\,s}^{-1})"
    },
    "temperature_pow":{
        "kind":"parameter",
        "value":5.0e3,
        "class":"parameter",
        "latex":"T_{exp~\\min}\\,{\\rm (K)}"
    },
    "temperature1":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{1~\\min}\\,{\\rm (K)}"
    },
    "fnickel1":{
        "kind":"parameter",
        "value":1.0e-1,
        "class":"parameter",
        "latex":"f_{1~\\rm Ni}"
    },
    "kappa12":{
        "kind":"parameter",
        "value":0.15,
        "class":"parameter",
        "latex":"\\kappa_{12}\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma12":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\_{12},({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "m1":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_1, (M_\\odot)"
    },
    "vejecta1":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{~1\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "temperature2":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{2~\\min}\\,{\\rm (K)}"
    },
    "fnickel2":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"f_{2~\\rm Ni}"
    },
    "m2":{
        "kind":"parameter",
        "value":0.1,
        "class":"parameter",
        "latex":"M_2, (M_\\odot)"
    },
    "vejecta2":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{2~\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "m3":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "m123":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "m1",
                "m2",
                "m3"
        ],
        "inputs":[
                "m1",
                "m2",
                "m3"
        ],
        "result":"m123",
        "class":"operator"
    },
    "m23":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "m2",
                "m3"
        ],
        "inputs":[
                "m2",
                "m3"
        ],
        "result":"m23",
        "class":"operator"
    },
    "pure_pow":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "tpeak",
            "lumscale",
            "texplosion",
            "alpha"
        ],
        "replacements":{
            "luminosities":"pure_pow_luminosities"
        }
    },
    "temperature_floor_pow":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "pure_pow",
            "temperature_pow",
            "vejecta_pow",
            "kappa_pow"
        ],
        "replacements":{
            "luminosities":"pure_pow_luminosities",
            "temperature":"temperature_pow",
            "vejecta":"vejecta_pow",
            "kappa":"kappa_pow"
        }
    },
    "blackbody_pow":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor_pow"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"pure_pow_luminosities"
        }
    },
    "nickelcobalt1":{
        "kind":"engine",
        "class":"nickelcobalt",
        "inputs":[
            "densetimes",
            "m1",
            "fnickel1",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities",
            "fnickel":"fnickel1",
            "mejecta":"m1"
        }
    },
    "diffusion1":{
        "kind":"transform",
        "class":"diffusion",
        "inputs":[
            "m1",
            "m2",
            "m3",
            "nickelcobalt1",
            "kappa12",
            "kappagamma12",
            "m123",
            "texplosion",
            "vejecta1"
        ],
        "replacements":{
            "mejecta":"m123",
            "luminosities":"nickel_cobalt1_luminosities",
            "kappa":"kappa12",
            "kappagamma":"kappagamma12",
            "vejecta":"vejecta1"
        }
    },
    "temperature_floor1":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "diffusion1",
            "temperature1"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities",
            "temperature":"temperature1",
            "fnickel":"fnickel1",
            "kappa":"kappa12",
            "kappagamma":"kappagamma12",
            "vejecta":"vejecta1"
        }
    },
    "blackbody1":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor1",
            "blackbody_pow"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt1_luminosities"
        }
    },
    "nickelcobalt2":{
        "kind":"engine",
        "class":"nickelcobalt",
        "inputs":[
            "densetimes",
            "m2",
            "fnickel2",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities",
            "fnickel":"fnickel2",
            "mejecta":"m2"
        }
    },
    "diffusion2":{
        "kind":"transform",
        "class":"diffusion",
        "inputs":[
            "m2",
            "m3",
            "nickelcobalt2",
            "kappa12",
            "kappagamma12",
            "m23",
            "texplosion",
            "vejecta2"
        ],
        "replacements":{
            "mejecta":"m23",
            "luminosities":"nickel_cobalt2_luminosities",
            "kappa":"kappa12",
            "kappagamma":"kappagamma12",
            "vejecta":"vejecta2"
        }
    },
    "temperature_floor2":{
        "kind":"photosphere",
        "class":"temperature_floor",
        "inputs":[
            "texplosion",
            "diffusion2",
            "temperature2"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities",
            "temperature":"temperature2",
            "fnickel":"fnickel2",
            "kappa":"kappa12",
            "kappagamma":"kappagamma12",
            "vejecta":"vejecta2"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor2",
            "blackbody1"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody2",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "losextinction",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt2_luminosities"
        }
    }
}
