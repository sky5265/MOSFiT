{
    "ebv":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "inputs":[
            "transient"
        ],
        "latex":"E({\\rm B} - {\\rm V})"
    },
    "fnickel":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"f_{\\rm Ni}"
    },
    "Re":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"r_e"
    },
    "vt":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"v_t"
    },
    "Me":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_e"
    },
    "nhhost":{
        "kind":"parameter",
        "value":1.0e18,
        "class":"parameter",
        "latex":"n_{\\rm H,host}",
        "derived_keys":"avhost"
    },
    "texplosion":{
        "kind":"parameter",
        "value":0.0,
        "class":"parameter",
        "latex":"t_{\\rm exp}\\,{\\rm (days)}",
        "requests":{
            "min_times": "transient"
        }
    },
    "temperature":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"T_{\\min}\\,{\\rm (K)}"
    },
    "kappa":{
        "kind":"parameter",
        "value":0.2,
        "class":"parameter",
        "latex":"\\kappa\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "kappagamma":{
        "kind":"parameter",
        "value":10.0,
        "class":"parameter",
        "latex":"\\kappa_\\gamma\\,({\\rm cm}^{2}\\,{\\rm g}^{-1})"
    },
    "mejecta":{
        "kind":"parameter",
        "value":1.0,
        "class":"parameter",
        "latex":"M_{\\rm ej}\\,(M_\\odot)"
    },
    "vejecta":{
        "kind":"parameter",
        "value":1.0e4,
        "class":"parameter",
        "latex":"v_{\\rm ej}\\,({\\rm km\\,s}^{-1})"
    },
    "m_total_diffusion":{
        "kind":"utility",
        "operator":"+",
        "operands":[
                "mejecta",
                "Me"
        ],
        "inputs":[
                "mejecta",
                "Me"
        ],
        "result":"m_total_diffusion",
        "class":"operator"
    },
    "nickelcobalt":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "mejecta",
            "fnickel",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "diffusion":{
        "kind":"transform",
        "inputs":[
            "nickelcobalt",
            "kappa",
            "kappagamma",
            "m_total_diffusion",
            "texplosion",
            "vejecta"
        ],
        "replacements":{
            "mejecta":"m_total_diffusion",
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "temperature_floor":{
        "kind":"photosphere",
        "inputs":[
            "texplosion",
            "diffusion",
            "temperature"
        ],
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "blackbody1":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "texplosion",
            "redshift",
            "temperature_floor"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"nickel_cobalt_luminosities"
        }
    },
    "shockcoolingpiro":{
        "kind":"engine",
        "inputs":[
            "densetimes",
            "Me",
            "Re",
            "vt",
            "kappa",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "piro_expanding_cooling_shock":{
        "kind":"photosphere",
        "inputs":[
            "shockcoolingpiro",
            "texplosion"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "blackbody2":{
        "kind":"sed",
        "class":"blackbody",
        "inputs":[
            "redshift",
            "piro_expanding_cooling_shock",
            "blackbody1"
        ],
        "requests":{
            "seds":"blackbody1",
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "losextinction":{
        "kind":"sed",
        "inputs":[
            "blackbody2",
            "nhhost",
            "rvhost",
            "ebv"
        ],
        "requests":{
            "band_wave_ranges": "photometry"
        },
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    },
    "photometry":{
        "kind":"observable",
        "supports":[
            "ultraviolet",
            "optical",
            "infrared"
        ],
        "inputs":[
            "losextinction",
            "lumdist"
        ],
        "replacements":{
            "luminosities":"piro_luminosities"
        }
    }
}
